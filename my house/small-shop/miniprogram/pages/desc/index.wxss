page{
  width: 100%;
  background: lightgray;
}
.topView{
  width: 750rpx;
  height: 300rpx;
  /* margin-top: 15px; */
  background: white;
}

.titleView{
  width: 100%;
  background: white;
  padding-top: 20rpx;
  padding-left: 20rpx;
  padding-bottom: 20rpx;
}
.title{
  width:650rpx;
}
.title text{
  color:black;
  font-size:13px;
  font-weight: 5px;

  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.bottmDesc{
  width: 710rpx;
  /* height: 40px; */
  position: relative;
  align-content: center;
}
.postalTip,.saleNum,.address{
  color: lightgray;
  display: inline-block;
  width: 233rpx;
  font-size: 11px;
}
.postalTip{
  text-align: left;
}
.saleNum{
  text-align: center;
}
.address{
  text-align: right;
}