.small_tip{
  height: 80rpx;
  clear: both;
  font-size: 12px;
  padding-left: 10px;
  background: white;
}
.small_tip .desc{
  color: black;
  height: 40rpx;
  font-size: 12px;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
}
.small_tip .price{
  color: rgb(188, 75, 89);
  font-size: 14px;
  height: 40rpx;
}
